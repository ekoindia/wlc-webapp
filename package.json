{
	"name": "eloka-webapp",
	"version": "4.39.0",
	"private": true,
	"description": "Eko's white-labelled financial platform service",
	"keywords": [
		"eko",
		"white-label"
	],
	"type": "module",
	"scripts": {
		"dev": "next dev -p 3002",
		"dev.https": "next dev -p 3004 --experimental-https",
		"scan": "next dev -p 3006 & npx react-scan@latest localhost:3006",
		"inspect": "NODE_OPTIONS='--inspect' next dev",
		"build": "next build",
		"start": "next start",
		"analyze": "ANALYZE=true next build",
		"eslint": "eslint . --config eslint.config.js",
		"eslint-quiet": "eslint . --config eslint.config.js --quiet",
		"eslint-fix": "eslint . --config eslint.config.js --fix",
		"eslint-errors": "eslint . --config eslint.config.js --quiet",
		"lint": "npm run eslint",
		"lint-error": "npm run eslint-quiet",
		"lint-quiet": "npm run eslint-quiet",
		"lint-fix": "npm run eslint-fix",
		"lint-staged": "lint-staged",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"g": "plop",
		"generate": "plop",
		"prepare": "husky",
		"sb": "storybook dev -p 6006",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"test": "jest",
		"test--raw": "jest",
		"test-failed-only": "jest --coverage=false --onlyFailures",
		"test-file": "jest --coverage=false --verbose",
		"test-file-pattern": "jest --coverage=false --testPathPattern",
		"test-watch": "jest --coverage=false --watch",
		"test-coverage": "jest --coverage",
		"test-update-snapshots": "jest --updateSnapshot",
		"check-updates": "npm-check -u",
		"check-deps": "node check-deps.js",
		"docker:dev": "docker compose up dev",
		"docker:prod": "docker compose up prod",
		"docker:dev:build": "docker compose up dev --build",
		"docker:prod:build": "docker compose up prod --build",
		"docker:stop": "docker compose down",
		"docker:clean": "docker compose down -v --rmi all"
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx}": [
			"prettier --write",
			"eslint --config eslint.config.js --fix"
		]
	},
	"overrides": {
		"eslint-config-next": {
			"eslint": "^9.4.0"
		}
	},
	"dependencies": {
		"@chakra-ui/icons": "^2.1.1",
		"@chakra-ui/react": "^2.8.2",
		"@copilotkit/react-core": "^1.9.1",
		"@copilotkit/react-ui": "^1.9.1",
		"@ekoindia/oaas-widget": "^2.5.0",
		"@emotion/react": "^11.10.5",
		"@emotion/styled": "^11.10.5",
		"@measured/puck": "^0.15.0",
		"@mediapipe/tasks-text": "^0.10.14",
		"@mediapipe/tasks-vision": "^0.10.14",
		"@next/third-parties": "^14.2.3",
		"@react-google-maps/api": "^2.18.1",
		"@react-oauth/google": "^0.11.0",
		"@types/node": "^20.12.12",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"date-fns": "^3.6.0",
		"framer-motion": "^11.2.6",
		"howler": "^2.2.4",
		"jsep": "^1.3.8",
		"jsonwebtoken": "^9.0.2",
		"kbar": "^0.1.0-beta.45",
		"lint": "^0.8.19",
		"next": "^14.2.20",
		"react": "^18.3.1",
		"react-color": "^2.19.3",
		"react-complex-tree": "^2.4.5",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.44.3",
		"react-icons": "^5.4.0",
		"react-image-crop": "^11.0.5",
		"react-markdown": "^9.0.1",
		"react-player": "^2.16.0",
		"react-select": "^5.8.0",
		"react-sounds": "^1.0.25",
		"react-webcam": "^7.2.0",
		"rehype-raw": "^7.0.0",
		"remark-gfm": "^4.0.0",
		"swr": "^2.1.0",
		"tinykeys": "^2.1.0",
		"uuid": "^10.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.24.6",
		"@chakra-ui/storybook-addon": "^5.2.5",
		"@chromatic-com/storybook": "^3.2.3",
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@eslint/compat": "^1.1.0",
		"@next/bundle-analyzer": "^14.2.3",
		"@next/eslint-plugin-next": "^14.2.3",
		"@storybook/addon-actions": "^8.4.7",
		"@storybook/addon-essentials": "^8.4.7",
		"@storybook/addon-interactions": "^8.4.7",
		"@storybook/addon-links": "^8.4.7",
		"@storybook/addon-mdx-gfm": "^8.4.7",
		"@storybook/nextjs": "^8.4.7",
		"@storybook/react": "^8.4.7",
		"@storybook/test": "^8.4.7",
		"@testing-library/dom": "^10.1.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^15.0.7",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^29.5.12",
		"@typescript-eslint/eslint-plugin": "^8.18.2",
		"@typescript-eslint/parser": "^8.18.2",
		"autoprefixer": "^10.4.19",
		"babel-loader": "^9.1.3",
		"chromatic": "^11.4.1",
		"eslint": "^9.4.0",
		"eslint-config-next": "^14.2.21",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jsdoc": "^48.2.6",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-storybook": "^0.11.1",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"lint-staged": "^15.2.7",
		"next-router-mock": "^0.9.13",
		"npm-check": "^6.0.1",
		"plop": "^4.0.1",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-organize-imports": "^3.2.4",
		"storybook": "^8.4.7",
		"typescript": "^5.4.5"
	}
}
