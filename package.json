{
	"name": "eko-infinity-webapp",
	"version": "4.5.0",
	"private": true,
	"description": "Eko's white-labelled financial platform service",
	"keywords": [
		"eko",
		"white-label"
	],
	"scripts": {
		"dev": "next dev -p 3002",
		"inspect": "NODE_OPTIONS='--inspect' next dev",
		"build": "next build",
		"start": "next start",
		"analyze": "ANALYZE=true next build",
		"lint": "next lint",
		"lint-fix": "next lint --fix",
		"lint-staged": "lint-staged",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"g": "plop",
		"generate": "plop",
		"prepare": "husky install",
		"sb": "start-storybook -p 6006",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"test": "jest",
		"pretest": "npm run lint",
		"test-file": "jest --coverage=false --verbose",
		"test-watch": "jest --coverage=false --watch",
		"test-coverage": "jest --coverage",
		"test-update-snapshots": "jest --updateSnapshot",
		"check-updates": "echo 'TO UPDATE, RUN: npm install package-name' && npm outdated --depth=0"
	},
	"resolutions": {
		"webpack": "^5",
		"@storybook/react/webpack": "^5"
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx}": [
			"prettier --write",
			"eslint --fix"
		]
	},
